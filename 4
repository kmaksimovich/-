number_complete_HW = 12
print(number_complete_HW)
number_hours_spent = 1.5
print(number_hours_spent)
name_course = 'Phyton'
TimeForOneTask = (number_hours_spent / number_complete_HW)
print(TimeForOneTask)
print('Курс:', name_course, 'всего задач:', number_complete_HW, 'затрачено часов:', number_hours_spent, 'среднее выполнение:', TimeForOneTask)
